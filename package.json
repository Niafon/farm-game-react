{
  "name": "farm-game-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "test:coverage": "jest --coverage --watchAll=false",
    "analyze": "vite build --mode analyze && echo Open bundle-stats.html",
    "audit": "npm audit --omit=dev && npx depcheck",
    "hh:compile": "hardhat compile",
    "hh:deploy": "hardhat run --network monad scripts/deploy.cjs",
    "hh:test": "hardhat test"
  },
  "dependencies": {
    "ethers": "^6.14.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "hardhat": "^2.22.15",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5",
    "@sentry/browser": "^8.35.0"
  }
}
